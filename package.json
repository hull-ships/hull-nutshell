{
  "name": "hull-nutshell",
  "version": "0.2.0",
  "description": "Hull Nutshell CRM integration",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run build:server",
    "build:server": "./node_modules/.bin/babel server -d lib",
    "start": "node -r newrelic ./lib",
    "start:dev": "NODE_ENV=development ./node_modules/.bin/nodemon -V -w server -x ./node_modules/.bin/babel-node -- server",
    "test": "npm run test:lint",
    "test:lint": "./node_modules/.bin/eslint server",
    "test:modules": "npm outdated --depth=0",
    "clean": "./node_modules/.bin/rimraf dist; ./node_modules/.bin/rimraf lib",
    "update": "./node_modules/.bin/updtr",
    "postinstall": "npm run build"
  },
  "engines": {
    "node": "6.x",
    "npm": "3.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hull-ships/hull-nutshell.git"
  },
  "keywords": [
    "hull",
    "ship",
    "nutshell",
    "hull-ship"
  ],
  "author": "Hull",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hull-ships/hull-nutshell/issues"
  },
  "homepage": "https://github.com/hull-ships/hull-nutshell",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bluebird": "^3.4.6",
    "bottleneck": "^1.13.1",
    "express": "^4.14.0",
    "hogan.js": "^3.0.2",
    "hull": "0.11.6",
    "lodash": "^4.14.1",
    "lru-cache": "^4.1.1",
    "newrelic": "^2.3.1",
    "request": "^2.72.0",
    "rimraf": "^2.5.4"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-register": "^6.26.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "nodemon": "^1.9.2",
    "updtr": "^0.1.15",
    "webpack": "^1.13.1"
  }
}
