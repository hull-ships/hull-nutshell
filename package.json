{
  "name": "hull-nutshell",
  "version": "0.3.0",
  "description": "Hull Nutshell CRM integration",
  "homepage": "https://github.com/hull-ships/hull-nutshell",
  "repository": {
    "type": "git",
    "url": "https://github.com/hull-ships/hull-nutshell.git"
  },
  "bugs": {
    "url": "https://github.com/hull-ships/hull-nutshell/issues"
  },
  "license": "MIT",
  "author": "Hull",
  "keywords": [
    "hull",
    "ship",
    "nutshell",
    "hull-ship"
  ],
  "engines": {
    "node": "^10.x",
    "npm": "5.6.x",
    "yarn": "^1.17.x"
  },
  "scripts": {
    "build": "yarn run clean && yarn run build:server",
    "build:server": "babel server -d build",
    "clean": "rimraf build",
    "test": "yarn run test:lint && yarn run test:combined",
    "test:combined": "NODE_ENV=test jest ./test",
    "test:unit": "NODE_ENV=test jest ./test/unit",
    "test:integration": "NODE_ENV=test jest ./test/integration",
    "test:flow": "flow check",
    "test:lint": "eslint server",
    "start": "node -r newrelic build",
    "start:dev": "NODE_ENV=development babel-watch -L server",
    "postinstall": "yarn run build"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "bluebird": "^3.7.2",
    "bottleneck": "^1.16.0",
    "cache-manager": "^2.11.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "hogan.js": "^3.0.2",
    "hull": "^0.13.23",
    "jayson": "^2.1.2",
    "json-rpc2": "^1.0.2",
    "lodash": "^4.17.21",
    "lru-cache": "^4.1.5",
    "moment": "^2.29.1",
    "newrelic": "^7.1.1",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.2",
    "babel-jest": "^21.2.0",
    "babel-watch": "^2.0.8",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-flowtype-errors": "^3.6.0",
    "eslint-plugin-import": "^2.22.1",
    "flow-bin": "^0.145.0",
    "flow-typed": "^2.6.2",
    "jest": "^21.2.1",
    "nock": "^9.6.1",
    "rimraf": "^2.7.1",
    "webpack": "^1.15.0"
  },
  "jest": {
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 10,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "transform": {
      "^.+\\.js?$": "babel-jest"
    }
  }
}
